* {
    outline: 1px dotted red;
}
.containers {
    width: 980px;
    min-height: 800px;
    background-image: radial-gradient(#6F5964, #1b0b0f);
}
    .header {
        background-color: whitesmoke;
        height: auto;
    }
        .header h1{
            display: inline-block;
            padding: 5px 15px;
            font-family: "Comic Sans MS", "Comic Sans", cursive;
            width: 465px;
        }
        .header h3{
            display: inline-block;
            padding: 5px 15px, 5px;
            font-family: Arial, sans-serif;
            text-align: right;
            width: 470px;
            
        }
        .main_top{
            width: 980px;
            min-height: 200px;
        }
            .main_top .photo{
                width: 410px;
                background-color: gray;
            }
            
            .main_top img{
                max-width: 400px;
                display: inline-block;
                vertical-align: auto;
            }
            .main_top .about{
                max-width: 450px;
                display: inline-block;
                vertical-align: auto;
            }
